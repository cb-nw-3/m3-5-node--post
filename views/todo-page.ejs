<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="/public/todo-list/style.css" />
  </head>
  <body>
    <div>
      <h1>My To Do List</h1>
    </div>
    <form method="POST" action="/data">
      <label>Enter an item</label>
      <input name="item" type="text" />
      <button type="submit">Submit</button>
    </form>
    <div>
      <ul>
        <% items.forEach(item => { %>
        <li><%= item %></li>
        <% }) %>
      </ul>
    </div>
  </body>
</html>
